import math

def factorize_rsa_number(n):
    factors = []
    
    # Check for divisibility by 2
    while n % 2 == 0:
        factors.append(2)
        n = n // 2
    
    # Check for divisibility by odd prime numbers
    for i in range(3, int(math.sqrt(n)) + 1, 2):
        while n % i == 0:
            factors.append(i)
            n = n // i
    
    # If n is still greater than 2, it is a prime number itself
    if n > 2:
        factors.append(n)
    
    return factors

# Example usage
rsa_number = 2497885147362973
factors = factorize_rsa_number(rsa_number)
prime_factors = [str(factor) for factor in factors]
print(f"{rsa_number} =", " * ".join(prime_factors))

